---
import Layout from "../layouts/Layout.astro";
import MaxWidth from "../layouts/MaxWidth.astro";
import Header from "../layouts/header/Header.astro";
import Title from "../components/Title.astro";
import Slider from "../components/agencia/Slider.astro";
import Pack from "../components/agencia/Pack.astro";
import Flex from "../components/agencia/Flex.astro";
import BtnContacto from "../components/BtnContacto.astro";
import Faqs from "../components/home/Faqs.astro";
import Contacto from "../components/Contacto.astro";
import Footer from "../layouts/Footer.astro";
---

<section class="overflow-hidden">
    <Layout title="Kaoticode | Agencia">
        <Header />
    
        <Title 
            title="agencia"
        />
    
        <Slider />
    
        <BtnContacto />
    
        <Pack />
    
        <Flex />
    
        <MaxWidth>
            <Faqs />
            <Contacto />
            <Footer />
        </MaxWidth>
    </Layout>

</section>